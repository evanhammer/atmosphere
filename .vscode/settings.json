{
  "editor.fontFamily": "InputMono-Thin",
  "editor.tabSize": 2,
  "editor.cursorStyle": "block",
  "editor.minimap.enabled": false,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "vim.insertModeKeyBindings": [],
  "vim.otherModesKeyBindingsNonRecursive": [],
  "window.restoreFullscreen": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "workbench.statusBar.visible": true,
  "explorer.confirmDelete": false,
  "git.confirmSync": false,
  "workbench.editor.labelFormat": "short",
  "workbench.editorAssociations": {
    "*.mdc": "default"
  },
  "editor.formatOnType": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 100,
  "workbench.editor.enablePreview": false,
  "gitlens.keymap": "alternate",
  "codemetrics.basics.CodeLensHiddenUnder": 7,
  "codemetrics.basics.ComplexityLevelHighDescription": "Who are you trying to impress?",
  "window.zoomLevel": 0,
  "codemetrics.basics.Exclude": ["**/*.spec.js*", "**/*.test.js"],
  "macros": {},
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "editor.formatOnSave": true,
  "files.autoSave": "afterDelay",
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.codeActionsOnSave": {
      "source.organizeImports.ruff": "explicit"
    }
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.colorTheme": "Deepdark Material Theme | Full Black Version",
  "workbench.preferredHighContrastColorTheme": "Deepdark Material Theme | Full Black Version",
  "eslint.lintTask.enable": true,
  "dart.flutterSdkPath": "/Users/evanhammer/Downloads/flutter",
  "eslint.useFlatConfig": true,
  "sqlfluff.dialect": "postgres",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "workbench.editor.revealIfOpen": true,
  "window.confirmSaveUntitledWorkspace": false,
  "sqlfluff.linter.run": "onSave",
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "terminal.external.osxExec": "iTerm.app",
  "gitlens.ai.model": "openai:gpt-4o",
  "breadcrumbs.showEvents": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "findUnusedExports.detectCircularImports": true,
  "findUnusedExports.considerMainExportsUsed": true,
  "window.customTitleBarVisibility": "windowed",
  "git.suggestSmartCommit": false
}
