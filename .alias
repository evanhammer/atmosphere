# COMMANDS
alias a="ack"
alias v="code"
alias vi="code"
alias l="less"
alias c="cat"
alias ll="ls -lhG"
alias la="ls -lhaG"
alias ps="ps -aef | grep -i"
alias lc="ls --ignore=\"*.pyc\""
